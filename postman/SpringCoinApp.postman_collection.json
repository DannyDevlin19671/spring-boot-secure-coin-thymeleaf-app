{
  "info": {
    "name": "Spring Coin App API",
    "_postman_id": "spring-coin-app-demo-collection",
    "description": "Postman collection for Spring Boot Coin API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Get All Coins",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/api/coins",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "api",
            "coins"
          ]
        }
      }
    },
    {
      "name": "Get All Mints",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseUrl}}/api/mints",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "api",
            "mints"
          ]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:8080"
    }
  ]
}